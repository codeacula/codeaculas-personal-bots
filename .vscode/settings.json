  "inlineChat.enableV2": true,
  "dotnet.defaultSolution": "Maestro.sln",
  "githubIssues.issueBranchTitle": "task/${issueNumber}-${sanitizedLowercaseIssueTitle}",
  "githubPullRequests.experimental.chat": false,
  "vue.autoInsert.dotValue": false,
}
